    "gulp-size": "^1.2.0",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.4",
    "gulp-webp": "^2.3.0",
    "gulp.spritesmith": "^3.5.4",
    "jshint-stylish": "^1.0.2",
    "pretty-hrtime": "^1.0.0",
    "require-dir": "^0.3.0",
