{
  "name": "gulp-workflow",
  "version": "1.0.0",
  "description": "Some workflow tools to help with building websites.",
  "private": true,
  "config": {
    "unsafe-perm":true
  },
  "browser": {
    "modernizr": "./app/_bower_components/modernizr/modernizr.js",
    "jquery": "./app/_bower_components/jquery/dist/jquery.js"
  },
  "browserify-shim": {
    "modernizr": "Modernizr",
    "jquery": "$"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "devDependencies": {
    "browser-sync": "^2.7.0",
    "browserify": "^10.1.0",
    "browserify-shim": "^3.8.6",
    "del": "^1.1.1",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^2.2.0",
    "gulp-base64": "^0.1.2",
    "gulp-cached": "^1.0.4",
    "gulp-filter": "^1.0.2",
    "gulp-gzip": "^1.1.0",
    "gulp-htmlmin": "^1.1.1",
    "gulp-imagemin": "^2.2.1",
    "gulp-jshint": "^1.10.0",
    "gulp-minify-css": "^1.1.1",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.0.0",
    "gulp-rev": "^3.0.1",
    "gulp-rev-collector": "^0.1.4",
    "gulp-rsync": "^0.0.5",
    "gulp-ruby-sass": "^1.0.5",
    "gulp-scss-lint": "^0.1.12",
